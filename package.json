{
  "name": "node-workhorse",
  "version": "0.0.0",
  "description": "A state-machine of sorts to delegate work efficiently and with resilience.",
  "dependencies": {
    "typings": "^0.6.8",
    "typescript": "^1.7.5",
    "tsc": "^1.20150623.0"
  },
  "main": "dist/index.js",
  "devDependencies": {
    "chai": "^3.5.0",
    "concurrently": "^1.0.0",
    "lite-server": "^2.0.1",
    "mocha": "^2.4.5",
    "typescript": "^1.7.5",
    "typings": "^0.6.8"
  },
  "scripts": {
    "postinstall": "npm run typings install && npm run dist-clean && npm run tsc",
    "tsc": "tsc",
    "tsc-test": "tsc -p test",
    "dist-clean": "rm -rf dist",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "start": "npm run dist-clean && concurrent \"npm run tsc:w\" \"npm run lite\" ",
    "pretest": "npm run dist-clean && npm run tsc-test",
    "test": "npm run mocha",
    "typings": "typings",
    "mocha": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/colinmathews/node-workhorse.git"
  },
  "author": "Colin Mathews",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/colinmathews/node-workhorse/issues"
  },
  "homepage": "https://github.com/colinmathews/node-workhorse#readme"
}
